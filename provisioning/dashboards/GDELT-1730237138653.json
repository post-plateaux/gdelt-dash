      "targets": [
        {
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "PCC52D03280B7034C"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT cc.country_description AS country_name, \n       COUNT(*) AS event_count\nFROM (\n    -- Actor 1 countries from both events tables\n    SELECT DISTINCT globaleventid, actor1countrycode AS country_code\n    FROM events\n    WHERE actor1countrycode IS NOT NULL\n\n    UNION ALL\n\n    SELECT DISTINCT globaleventid, actor1countrycode AS country_code\n    FROM events_translated\n    WHERE actor1countrycode IS NOT NULL\n\n    UNION ALL\n\n    -- Actor 2 countries from both events tables\n    SELECT DISTINCT globaleventid, actor2countrycode AS country_code\n    FROM events\n    WHERE actor2countrycode IS NOT NULL\n      AND actor2countrycode <> actor1countrycode\n\n    UNION ALL\n\n    SELECT DISTINCT globaleventid, actor2countrycode AS country_code\n    FROM events_translated\n    WHERE actor2countrycode IS NOT NULL\n      AND actor2countrycode <> actor1countrycode\n) AS combined_countries\nJOIN cameo_country cc ON combined_countries.country_code = cc.country_code\nGROUP BY cc.country_description\nORDER BY event_count DESC;\n",
          "refId": "A",
